// This file is encoded as Windows 1252 to ensure the copyright symbol appears

#define VFT_APP 0x00000001L
#define VOS_NT  0x00040000L
#define VS_FF_DEBUG 0x00000001L

#define ODASHIM_DESC "Odashim"
#define ODASHIM_EXE "odashim.exe"
#define ODASHIM_INTERNAL "odashim"
#define ODASHIM_NAME "Odashim"

1 ICON "@CMAKE_SOURCE_DIR@/shim/win32/odashim.ico"

1 VERSIONINFO
FILEVERSION @PROJECT_RC_VERSION@
PRODUCTVERSION @PROJECT_RC_VERSION@
FILEFLAGSMASK 0
#ifdef _DEBUG
FILEFLAGS VS_FF_DEBUG
#else
FILEFLAGS 0
#endif
FILEOS VOS_NT
FILETYPE VFT_APP
FILESUBTYPE 0
{
  BLOCK "StringFileInfo"
  {
    // US English, Windows MultiLingual characterset
    BLOCK "040904E4"
    {
      VALUE "CompanyName", "@PROJECT_COMPANY@"
      VALUE "FileDescription", ODASHIM_DESC
      VALUE "FileVersion", "@PROJECT_VERSION@"
      VALUE "InternalName", ODASHIM_INTERNAL
      VALUE "LegalCopyright", "ODAMEX and its likeness are Copyright © @PROJECT_COPYRIGHT@ by @PROJECT_COMPANY@."
      VALUE "OriginalFilename", ODASHIM_EXE
      VALUE "ProductName", ODASHIM_NAME
      VALUE "ProductVersion", "@PROJECT_VERSION@"
    }
  }
  BLOCK "VarFileInfo"
  {
    // U.S. English, Multilingual
    VALUE "Translation", 0x0409, 1252
  }
}
